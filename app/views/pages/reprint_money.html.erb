<style>
th, td {
padding: 5px;
} 

</style>

<table>
<tr>
<th>Id</th>
<th>Name</th>
<th>Company</th>
<th>From</th>
<th>Country</th>
<th>Amount</th>

<th>Action</th>

</tr>

<div class="row" ><span class="col-11" ></span>
<%= link_to "Home", index_path, class: "col-1 btn btn-info" %></div>
<% @money.each do |en| %>
  <tr>
    <td><%= en.id %></td>
    <td><%= en.name %></td>
    <td><%= en.company %></td>
    <td><%= en.from %></td>
    <td><%= en.country %></td>
    <td><%= en.amount %></td>
    <td><%= link_to "Print", print_money_path(en,1), class: "btn btn-success" %></td>
  </tr>
<% end %>

</table>