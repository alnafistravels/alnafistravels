<div class="row" ><span class="col-11" ></span>
<%= link_to "Home", index_path, class: "col-1 btn btn-info" %></div>
  <div class="d-flex justify-content-center align-items-center" >
      <h1>Others Bill Printing</h1>
    </div>

<%= form_for @entry, url: entry_update_path ,html: {class: 'p-5 m-5 container form-group'} do |f| %>
  
  <div class="row" >
  <div class="col-3">
  <%= f.label :name %>
  </div><div class="col-6" >
  <%= f.text_field :name, multiple: true, class: "form-control " %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3">
  <%= f.label :from %>
  </div><div class="col-6" >
  <%= f.text_field :from, class: "form-control" %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3">
  <%= f.label :service_name %>
  </div><div class="col-3" >
  <%= f.select :service_name, @s, {}, {class: "form-control"} %>
  </div><div class="col-3" >
  <%= f.text_field :days, class: "form-control" %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3"> 
  <%= f.label :to, "Country" %>
  </div><div class="col-6" >
  <%= f.select :to , @places, {}, {class: "form-control"} %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3"> 
  <%= f.label :price, "Total" ,class:"col-2" %>
  </div><div class="col-6" >
  <%= f.number_field :price, class: "col-4 form-control" %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3"> 
  <%= f.label :paid, class:"col-2" %>
  </div><div class="col-6" >
  <%= f.number_field :paid, class: "col-4 form-control" %>
  </div>
  </div>
  <br>
  <div class="row" >
  <div class="col-3" >
  <%= f.label :phone_number %>
  </div><div class="col-6" ><%= f.text_field :phone, class:"form-control" %>
  </div>
  <br>
  </div>
  <br>
  <div class="row"><div class="col-7" ></div><div class="col-2" >
  <%= f.submit "Submit" , class: "btn btn-success form-control" %>
  </div></div>

<% end %>